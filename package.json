  "react-native": {
    "fs": "react-native-level-fs"
  },
  "browser": {
    "fs": "react-native-level-fs"
  },
  "lint-staged": {
    "source/**/*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "babel": {
    "env": {
      "test": {
        "plugins": [
          "babel-plugin-transform-es2015-modules-commonjs",
          "babel-plugin-transform-flow-strip-types",
          "babel-plugin-transform-class-properties",
          [
            "babel-plugin-transform-object-rest-spread",
            {
              "useBuiltIns": true
            }
          ]
        ],
        "presets": [
          "jest"
        ]
      }
    }
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "lcov"
    ],
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "source/**/*.js",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "modulePathIgnorePatterns": [
      "\\/\\."
    ]
  },
  "scripts": {
    "start": "npm run build",
    "build": "node --max-old-space-size=8192 node_modules/rollup/dist/bin/rollup -c rollup.config.js",
    "build:watch": "rollup --config --watch \"./source\"",
    "lint": "./node_modules/.bin/eslint \"source/**/*.js\"",
    "flow": "./node_modules/.bin/flow",
    "server": "node --max-old-space-size=8192 node_modules/rollup/dist/bin/rollup -c rollup.serve.js",
    "test": "jest --colors",
    "test:watch": "jest --colors --watch",
    "lint-staged": "lint-staged",
    "precommit": "npm run build && npm run test && npm run lint-staged"
  }
}
